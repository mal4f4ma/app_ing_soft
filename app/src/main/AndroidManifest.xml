<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jesusdominguez.hayari_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AvisoPrivacidad"
            android:label="@string/title_activity_aviso_privacidad"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Config" />
        <activity
            android:name=".MenuJuegos"
            android:label="@string/title_activity_menu_juegos"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Juego1" />
        <activity
            android:name=".Juego2"
            android:label="@string/title_activity_juego2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Juego3"
            android:label="@string/title_activity_juego3"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>